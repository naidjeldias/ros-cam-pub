<launch>
    <node name="cam_pub_node" pkg="cam_pub" type="cam_pub_node" output="screen">
        <param name="video_device" value="/dev/video1" />
        <param name="devType" value="stereo" />
        <param name="pixel_format" value="rgb" />
        <param name="autofocus" value="false" />
        <param name="focus" value="0" />
        <param name="autoexposure" value="false" />
        <param name="exposure" value="88" />
        <param name="auto_white_balance" value="false" />
        <param name="white_balance" value="4077" />
    </node>

</launch>